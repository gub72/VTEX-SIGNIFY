{
  // Blocs
  "store.custom#politica-de-privacidade": {
    "blocks": ["flex-layout.row#breadcrumb-row", "flex-layout.row#main-row"]
  },
  // breadcrumb
  "flex-layout.row#breadcrumb-row": {
    "children": ["flex-layout.col#breadcrumb"],
    "props": {
      "blockClass": "containerInstitucional",
      "fullWidth": false,
      "horizontalAlign": "center"
    }
  },
  "flex-layout.col#breadcrumb": {
    "children": ["rich-text#breadcrumb-list"],
    "props": {
      "blockClass": "containerInstitucional__breadcrumb",
      "width": "100%"
    }
  },
  "rich-text#breadcrumb-list": {
    "props": {
      "text": "[Home](/) [Institucional](#) [Politica de privacidade](#)",
      "blockClass": "containerInstitucional__breadcrumb-list"
    }
  },
  // Main
  "flex-layout.row#main-row": {
    "children": ["flex-layout.col#aside", "flex-layout.col#contentPolitica"],
    "props": {
      "blockClass": "containerInstitucional",
      "fullWidth": false,
      "horizontalAlign": "center"
    }
  },

  // aside
  "flex-layout.col#aside": {
    "children": [
      "rich-text#aside-tittle",
      "vtex.menu@2.x:menu#menu__institucional"
    ],
    "props": {
      "blockClass": "institucional__menu-container",
      "id": "institucional__menu",
      "width": "20%"
    }
  },
  "rich-text#aside-tittle": {
    "props": {
      "text": "###### **Título**",
      "blockClass": "institucional__menu-container-tittle"
    }
  },

  // content
