    "children": ["flex-layout.col#contentPolitica-text"],
    "props": {
      "width": "80%"
    }
  },
  "flex-layout.col#contentPolitica-text": {
    "children": [
      "rich-text#contentPolitica-tittle-1",
      "rich-text#contentPolitica-paragraph-1",
      "rich-text#contentPolitica-paragraph-1-2",
      "rich-text#contentPolitica-tittle-2",
      "rich-text#contentPolitica-paragraph-2"
    ],
    "props": {
      "blockClass": "textColumn",
      "preventVerticalStretch": true
    }
  },
